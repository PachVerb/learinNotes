#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_browserify@12.0.2@browserify/bin/cmd.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_browserify@12.0.2@browserify/bin/cmd.js" "$@"
  ret=$?
fi
exit $ret
