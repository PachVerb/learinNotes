#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_buble@0.16.0@buble/bin/buble" "$@"
  ret=$?
else 
  node  "$basedir/../../../_buble@0.16.0@buble/bin/buble" "$@"
  ret=$?
fi
exit $ret
